         }
      },
      "desktop-text-capture-instant": {
         "description": "Desktop Text Capture (Instant)",
         "suggested_key": {
            "default": "Ctrl+Shift+I",
            "mac": "Command+Shift+I"
         }
      },
      "get-image-from-clipboard": {
         "description": "Get image from clipboard",
         "suggested_key": {
            "default": "Ctrl+Shift+C",
            "mac": "Command+Shift+C"
         }
      }
